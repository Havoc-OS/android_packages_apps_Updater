}

prebuilt_etc {
    name: "privapp_whitelist_com.havoc.updater",
    src: "prebuilt/etc/permissions/privapp_whitelist_com.havoc.updater.xml",
    sub_dir: "permissions",
    filename_from_src: true,
}
