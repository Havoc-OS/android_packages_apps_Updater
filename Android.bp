android_app_import {
	name: "Updater",
	apk: "prebuilt/priv-app/Updater.apk",
	certificate: "platform",
	dex_preopt: {
		enabled: false,
	},
	privileged: true,
    required: [
        "privapp_whitelist_com.havoc.updater",
